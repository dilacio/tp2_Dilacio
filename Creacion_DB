/****** Object:  Table [dbo].[Articulo]    Script Date: 15/9/2019 16:55:05 ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO

CREATE TABLE [dbo].[Articulo](
	[ID] [int] IDENTITY(1,1) NOT NULL,
	[Nombre] [varchar](50) NOT NULL,
	[CodigoArticulo] [int] NOT NULL,
	[Descripcion] [varchar](100) NOT NULL,
	[ID_Marca] [int] NOT NULL,
	[ID_Categoria] [int] NOT NULL,
	[Precio] [float] NOT NULL,
	[Imagen] [varchar](200) NULL,
	[Baja] [int] NULL,
PRIMARY KEY CLUSTERED 
(
	[ID] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO

ALTER TABLE [dbo].[Articulo]  WITH CHECK ADD FOREIGN KEY([ID_Categoria])
REFERENCES [dbo].[Categoria] ([ID])
GO

ALTER TABLE [dbo].[Articulo]  WITH CHECK ADD FOREIGN KEY([ID_Marca])
REFERENCES [dbo].[Marca] ([ID])
GO

USE [Negocio]
GO

